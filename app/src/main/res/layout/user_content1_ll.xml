<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:divider="@drawable/divider"
    android:showDividers="middle">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        >
        <ImageView
            android:id="@+id/iv1"
            android:layout_width="30dip"
            android:layout_height="30dip"
            android:src="@drawable/member_level_icon"
            android:layout_centerVertical="true"
            android:layout_margin="10dip"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/turn_right"
            android:layout_margin="10dip"
            android:layout_centerVertical="true"
            android:id="@+id/imageView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="会员等级礼品"
            android:textColor="@color/theme_gray"
            android:layout_toEndOf="@+id/iv1"
            android:textSize="18sp"
            android:id="@+id/textView"
            android:layout_centerVertical="true"
            android:gravity="center"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        >
        <ImageView
            android:id="@+id/iv2"
            android:layout_width="30dip"
            android:layout_height="30dip"
            android:src="@drawable/mine_paypeople_icon"
            android:layout_centerVertical="true"
            android:layout_margin="10dip"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="常用信息"
            android:textColor="@color/theme_gray"
            android:layout_toRightOf="@id/iv2"
            android:layout_centerVertical="true"
            android:textSize="18sp"
             />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/turn_right"
            android:layout_margin="10dip"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        >
        <ImageView
            android:id="@+id/iv3"
            android:layout_width="30dip"
            android:layout_height="30dip"
            android:src="@drawable/mine_favorite"
            android:layout_margin="10dip"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的收藏"
            android:textColor="@color/theme_gray"
            android:layout_toRightOf="@id/iv3"
            android:textSize="18sp"
            android:layout_centerVertical="true"
            android:gravity="center" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/turn_right"
            android:layout_margin="10dip"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        >
        <ImageView
            android:id="@+id/iv4"
            android:layout_width="30dip"
            android:layout_height="30dip"
            android:src="@drawable/mine_travel_icon"
            android:layout_centerVertical="true"
            android:layout_margin="10dip"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的游记"
            android:textColor="@color/theme_gray"
            android:layout_toRightOf="@id/iv4"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:textSize="18sp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/turn_right"
            android:layout_margin="10dip"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        >
        <ImageView
            android:id="@+id/iv5"
            android:layout_width="30dip"
            android:layout_height="30dip"
            android:src="@drawable/offline_travel_icon"
            android:layout_centerVertical="true"
            android:layout_margin="10dip"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="离线游记"
            android:textColor="@color/theme_gray"
            android:layout_toRightOf="@id/iv5"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:textSize="18sp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/turn_right"
            android:layout_margin="10dip"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        >
        <ImageView
            android:id="@+id/iv6"
            android:layout_width="30dip"
            android:layout_height="30dip"
            android:src="@drawable/mine_browse_history"
            android:layout_centerVertical="true"
            android:layout_margin="10dip"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="历史浏览"
            android:textColor="@color/theme_gray"
            android:layout_toRightOf="@id/iv6"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:textSize="18sp"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/turn_right"
            android:layout_margin="10dip"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

</LinearLayout>
